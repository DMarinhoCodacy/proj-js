{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "es6": true,
    "worker": true
  },
  "parserOptions": {
    "ecmaVersion": 10,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-unused-vars": "warn",
    "class-methods-use-this": "warn"
  },
  "ignorePatterns": [
    "**/node_modules/**/*",
    "**/dist/**/*",
    "**/bin/**/*",
    "**/build/**/*",
    "**/docs/tests/**/*",
    "tsconfig.json"
  ]
}