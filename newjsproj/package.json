    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^7.0.0",
    "serverless-pseudo-parameters": "^2.4.0",
    "serverless-stage-manager": "^1.0.0",
    "serverless-webpack": "^5.3.0",
    "serverless-esbuild": "^1.23.3",
    "webpack": "^4.32.0",
    "webpack-node-externals": "^1.7.0",
    "@polestar/quality-cli": "^3.33.0",
    "@polestar/build-configs-api": "^4.1.0",
    "@polestar/config-utils": "^1.2.0",
    "@polestar/eslint-config-api": "^0.5.0",
    "@polestar/test-configs-api": "^0.8.1",
    "@polestar/tsconfig-api": "^0.4.0",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{documentation,runbooks}**/*.{md}": [
      "prettier --write",
      "git add"
    ],
    "{functions,schema,data-sources}**/*.{js,mjs}": [
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "handlebars": ">=4.3.0",
    "https-proxy-agent": ">=3.0.0",
    "eslint-utils": ">=1.4.1",
    "lodash": ">=4.17.12",
    "lodash.merge": ">=4.6.2"
  }
}