  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/{app,config}/**/*.{js,jsx,mjs}": [
      "prettier --write",
      "git add"
    ],
    "tests/**/*.{js,jsx,mjs}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "@polestar/eslint-config-frontend",
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": [
        "error"
      ]
    }
  }
