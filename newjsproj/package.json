{
  "name": "vs-b2b-order-api",
  "version": "0.0.1",
  "description": "Polestar GraphQL API powered by AWS",
  "private": true,
  "license": "UNLICENSED",
  "keywords": [
    "apollo",
    "graphql",
    "lambda",
    "serverless",
    "api"
  ],
  "scripts": {
    "debug": "node --inspect ./node_modules/serverless/bin/serverless offline start",
    "dev": "sls offline start",
    "postinstall": "cpy --no-overwrite --rename .env .env.example .",
    "test": "jest",
    "prequality-check": "yarn test",
    "quality-check": "devhouse-quality-check",
    "quality-check:browser": "yarn quality-check --browser"
  },
  "dependencies": {
    "@polestar/dotenv-utils": "^1.1.0",
    "@polestar/feature-flags": "^1.3.0",
    "@polestar/graphql-server": "^6.33.0",
    "@polestar/lambda-logger": "^1.2.1",
    "@polestar/lambda-utils": "^1.1.0",
    "apollo-server-lambda": "^2.9.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql": "^14.7.0",
    "graphql-type-json": "^0.3.0",
    "node-fetch": "^2.6.0",
    "@aws-sdk/protocol-http": "3.160.0",
    "@aws-sdk/signature-v4": "3.160.0",
    "@aws-sdk/credential-provider-node": "3.160.0",
    "@aws-crypto/sha256-js": "2.0.1",
    "cross-fetch": "^3.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.0",
    "apollo-server-testing": "^2.25.3",
    "aws-sdk": "^2.466.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-jest": "^22.16.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^9.2.0",
    "plop": "^2.3.0",
    "prettier": "^1.18.0",
    "prettier-eslint": "^12.0.0",
