    "uuid": "^8.3.2",
    "wait-for-stuff": "^1.4.0"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "reporter": [
      "text",
      "lcov"
    ],
    "exclude": [
      "**/test/**",
      "**/types/**",
      "coverage/**",
      "index.*"
    ]
  }
}