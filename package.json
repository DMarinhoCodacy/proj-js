{
  "name": "gametime",
  "version": "2023.17.0",
  "private": true,
  "scripts": {
    "postinstall": "npx jetifier && patch-package",
    "precommit": "yarn build",
    "start": "react-native start",
    "build": "tsc",
    "test": "jest",
    "lint_covered": "eslint . --ext .tsx --ext .ts --quiet -c .eslintrc.cicd.js",
    "lint": "eslint . --ext .tsx --ext .ts",
    "open_url": "bash ./scripts/open_url.sh",
    "test-coverage": "jest  --coverage",
    "watchTests": "jest --watch",
    "react-devtools": "react-devtools",
    "clean_android": "bash ./scripts/clean_android.sh",
    "clean_ios": "bash ./scripts/clean_ios.sh",
    "run_ios": "bash ./scripts/run_ios.sh",
    "run_ios_detox": "bash ./scripts/run_ios_detox.sh",
    "setup_ios": "bash ./scripts/setup_ios.sh",
    "crank_ios": "yarn setup_ios && yarn run_ios",
    "build:android-detox": "detox build --configuration android.emu.release",
    "build:ios-detox": "detox build --configuration ios.sim.release",
    "test:android-detox": "detox test --configuration android.emu.release",
    "test:ios-detox": "detox test --configuration ios.sim.release",
    "ragnarok": "bash ./scripts/ragnarok.sh",
    "check-deps": "rnx-align-deps"
  },
  "ts-standard": {
    "project": "./tsconfig.json"
  },
  "dependencies": {
