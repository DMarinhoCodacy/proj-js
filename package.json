{
  "name": "mainapp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "watch-tsc": "tsc -w",
    "dev": "nodemon index.local.js",
    "server": "concurrently \"npm run watch-tsc\" \"npm run dev\"",
    "test": "tsc && mocha",
    "post:test": "tsc && nyc mocha",
    "build": "tsc --version && tsc --removeComments --skipLibCheck",
    "build:dev": "tsc -w",
    "postinstall": "npm dedupe",
    "pre-commit": "pretty-quick --staged --pattern \"**/*.*ts\" --verbose --bail",
    "lint": "tslint --project tsconfig.json --fix",
    "coverage": "tsc && nyc mocha --check leaks --exit",
    "migrate:up-dev": "migrate-mongo up",
    "migrate:up-test": "migrate-mongo up -f migrate-mongo-config-test.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/aws-lambda": "^8.10.85",
    "@types/aws-serverless-express": "^3.3.5",
    "@types/chai": "^4.2.22",
    "@types/chai-http": "^4.2.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/flat": "5.0.2",
    "@types/hapi__joi": "^17.1.7",
    "@types/helmet": "0.0.48",
    "@types/http-status-codes": "^1.2.0",
    "@types/keycloak-connect": "^4.5.4",
    "@types/lodash": "^4.14.177",
    "@types/mocha": "^9.0.0",
    "@types/mocha-prepare": "^0.1.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.11",
    "@types/nodemailer": "^6.4.4",
    "@types/number-to-words": "^1.2.1",
    "@types/request-promise": "^4.1.48",
    "@types/sanitize-html": "^2.6.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "chai": "^4.3.4",
    "concurrently": "^6.4.0",
    "husky": "^7.0.4",
    "lodash": "^4.17.21",
    "mocha": "^9.1.3",
    "mocha-prepare": "^0.1.0",
    "mongo-unit": "^2.0.1",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "dependencies": {
    "@hapi/joi": "^16.1.7",
    "aws-lambda": "^1.0.7",
    "aws-serverless-express": "^3.4.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "flat": "^5.0.2",
    "fuzzball": "^2.1.2",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.2.0",
    "isomorphic-fetch": "^3.0.0",
